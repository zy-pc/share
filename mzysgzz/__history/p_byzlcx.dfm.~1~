    Connection = DM_data.ado_mydata
    CursorType = ctStatic
    LockType = ltReadOnly
    CommandText = 
      'select * from icd10 where (jbmc like :jbmc or pybm like :pym or ' +
      'jbbm like :jbbm) and charindex('#39'>'#39',jbbm)=0 order by jbbm'
    Parameters = <
      item
        Name = 'jbmc'
