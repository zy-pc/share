  object ado_qry: TADOQuery
    Connection = DM_data.ado_mydata
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'ksdm'
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = 4
        Value = Null
      end
      item
        Name = 'ksrq'
        DataType = ftDateTime
        NumericScale = 3
        Precision = 23
        Size = 16
        Value = Null
      end
      item
        Name = 'jsrq'
        DataType = ftDateTime
        NumericScale = 3
        Precision = 23
        Size = 16
        Value = Null
      end>
    SQL.Strings = (
      
        'select a.zyh,a.tmh,a.brxm,a.brxb,a.brnl,c.mc as ksmc,a.ryrq,a.ry' +
        'zd,d.mc as ysxm,a.zgys as ysdm,a.zfy,DATEDIFF(day,ryrq,ISNULL(cy' +
        'rq,getdate())) as ts,F.BZFY,f.BZZYR,a.ksdm'
      
        ' from zysf_zydj a,LCLJ_brlclj b,sys_ksdm c,sys_czy d,LCLJ_LCLJML' +
        ' e,LCLJ_LCLJBB F'
      
        '  where a.zyh=b.zyh and a.ksdm=c.dm and a.zgys=d.dm and b.LJID=e' +
        '.LJID '
      
        '  and ISNULL(e.dbzgl,0)=1 AND E.LJID=F.LJID and c.dm=:ksdm and  ' +
        'ryrq >= :ksrq and  ryrq <= :jsrq')
    Left = 456
    Top = 176
